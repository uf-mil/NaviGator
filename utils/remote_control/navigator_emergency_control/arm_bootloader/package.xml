<package>
  <name>arm_bootloader</name>
  <version>1.0.0</version>
  <description>arm_bootloader</description>
  <maintainer email="forrest@forre.st">Forrest Voight</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/arm_bootloader</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Forrest Voight</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>libopencm3</build_depend>
  <build_depend>uf_subbus_protocol</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>libopencm3</run_depend>
  <run_depend>uf_subbus_protocol</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>roscpp</test_depend> -->
  <!-- <test_depend>libopencm3</test_depend> -->
  <!-- <test_depend>uf_subbus_protocol</test_depend> -->

<export>
    <cpp cflags="-I${prefix}/include"/>
</export>

</package>